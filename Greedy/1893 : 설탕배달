#include <iostream>

using namespace std;

int main()
{
	int n;

	cin >> n;

	int a = n / 5;		// 5로 나눈 몫
	int b;				

	for (; a >= 0; a--)		// 그리디인 이유 : 현 상황에서 5키로 포대가 많으면
				// 결과가 좋을 것이기 때문에 5키로 포대가 제일 많을 때 부터
				// 해보기 때문.
	{
		b = 0;

		if (n - (a * 5) == 0) break;

		b = (n - (a * 5)) / 3;

		if ((n - (a * 5)) % 3 == 0)
		{

			break;
		}

		if (a == 0)			// a=0에서도 break를 못했기 때문에 불가한 경우.
		{
			cout << -1;
			return 0;
		}

		
	}

	cout << a + b;
}
